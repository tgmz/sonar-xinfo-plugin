<mc-pattern>
	<mc>
		<key>MC00001</key>
		<regex lang="all" casesensitive="false">^.*\brexx.*$</regex>
	</mc>
	<mc>
		<key>MC00002</key>
		<regex lang="all" casesensitive="false">^.*https?|ftp://.*$</regex>
	</mc>
	<mc>
		<key>MC00003</key>
		<regex lang="all" casesensitive="false">^.*idmsock|socket.*$</regex>
	</mc>
	<mc>
		<key>MC00004</key>
		<regex lang="all" casesensitive="false">^.*\bY[0-9]E?[0-9]{4}\b.*$</regex>
		<regex lang="all" casesensitive="false">^.*\bYA[0-9]E?[0-9]{4}\b.*$</regex>
		<regex lang="all" casesensitive="false">^.*\bbab[0-9]{4}\b.*$</regex>
	</mc>
	<mc>
		<key>MC00005</key>
		<regex lang="pli,mac" casesensitive="false">^.*\s+%noprint.*$</regex>
	</mc>
	<mc>
		<key>MC00006</key>
		<regex lang="all" casesensitive="false">^.*\bsecret.*$</regex>
		<regex lang="all" casesensitive="false">^.*\bpassw.*$</regex>
	</mc>
	<mc>
		<key>MC00007</key>
		<regex lang="all" casesensitive="false">^.*csf.*$</regex>
	</mc>
	<mc>
		<key>MC00008</key>
		<regex lang="all" casesensitive="false">^.*\bdrop\s+table\b.*$</regex>
	</mc>
	<mc>
		<key>MC00009</key>
		<regex lang="all" casesensitive="false">^.*(\b(0?[1-9]|[12]\d|30|31)[^\w\d\r\n:](0?[1-9]|1[0-2])[^\w\d\r\n:](\d{4}|\d{2})\b)|(\b(0?[1-9]|1[0-2])[^\w\d\r\n:](0?[1-9]|[12]\d|30|31)[^\w\d\r\n:](\d{4}|\d{2})\b).*$</regex>
		<regex lang="all" casesensitive="false">^.*\b(20[1-3]\d{1})(0[1-9]|1[0-2])(0[1-9]|[12]\d|30|31)\b.*$</regex>
		<regex lang="all" casesensitive="false">^.*\b([0-3]\d{1})\.(0[1-9]|1[0-2])\.([1|2]\d{3})\b.*$</regex>
		<regex lang="all" casesensitive="false">^.*\b(\d{4})\-(0[1-9]|1[0-2])\-(0[1-9]|[12]\d|30|31)\b.*$</regex>
	</mc>                                        
	<mc>
		<key>MC00010</key>
		<regex lang="all" casesensitive="false">^.*\brollb.*$</regex>
	</mc>
	<mc>
		<key>MC00011</key>
		<regex lang="pli,mac" casesensitive="false">^.*\bput.*\(.*_rating\b.*$</regex>
		<regex lang="pli,mac" casesensitive="false">^.*\bput.*\(.*_betr\b.*$</regex>
		<regex lang="pli,mac" casesensitive="false">^.*\bput.*\(.*_proz\b.*$</regex>
		<regex lang="pli,mac" casesensitive="false">^.*\bput.*\(.*_add?r\b.*$</regex>
		<regex lang="cbl,mac" casesensitive="false">^.*\bwrite.*\-rating\b.*$</regex>
		<regex lang="cbl,mac" casesensitive="false">^.*\bwrite.*\-betr\b.*$</regex>
		<regex lang="cbl,mac" casesensitive="false">^.*\bwrite.*\-proz\b.*$</regex>
		<regex lang="cbl,mac" casesensitive="false">^.*\bwrite.*\-add?r\b.*$</regex>
		<regex lang="cbl,mac" casesensitive="false">^.*\bprint.*\-rating\b.*$</regex>
		<regex lang="cbl,mac" casesensitive="false">^.*\bprint.*\-betr\b.*$</regex>
		<regex lang="cbl,mac" casesensitive="false">^.*\bprint.*\-proz\b.*$</regex>
		<regex lang="cbl,mac" casesensitive="false">^.*\bwrite.*\-add?r\b.*$</regex>
		<regex lang="sas" casesensitive="false">^.*\bput.*rating\b*$</regex>
		<regex lang="sas" casesensitive="false">^.*\bput.*betr\b*$</regex>
		<regex lang="sas" casesensitive="false">^.*\bput.*proz\b*$</regex>
		<regex lang="sas" casesensitive="false">^.*\bput.*add?r.*$</regex>
	</mc>
	<mc>
		<key>MC00012</key>
		<regex lang="all" casesensitive="false">^.*\b((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\b.*$</regex>
	</mc>
	<mc>
		<key>MC00013</key>
		<regex lang="all" casesensitive="false">^.*\bTmpBlb\b.*$</regex>
	</mc>
	<mc>
		<key>MC00016</key>
		<regex lang="all" casesensitive="true">^.*\b([0-9a-zA-Z]([\+\-_\.][0-9a-zA-Z]+)*)+@(([0-9a-zA-Z][-\w]*[0-9a-zA-Z]*\.)+[a-zA-Z0-9]{2,17})\b.*$</regex>
	</mc>
	<mc>
		<key>MC00042</key>
		<regex lang="all" casesensitive="false">^.*\bexecute\s+immediate\b.*$</regex>
	</mc>
</mc-pattern>
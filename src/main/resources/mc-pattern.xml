<mc-pattern>
	<mc>
		<key>MC00016</key>
		<description>EmailSpying</description>
		<regex>^.*([0-9a-zA-Z]([\+\-_\.][0-9a-zA-Z]+)*)+@(([0-9a-zA-Z][-\w]*[0-9a-zA-Z]*\.)+[a-zA-Z0-9]{2,17}).*$</regex>
	</mc>
	<mc>
		<key>MC00042</key>
		<description>StaticSQLQuery</description>
		<regex>^.*\\s+IMMEDIATE\\s+.*$</regex>
	</mc>
</mc-pattern>